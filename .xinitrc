exec xmonad &

# Setting keyboard repeat rate
xset r rate 300 40 &

# Set keyboard layout to dvorak
setxkbmap us -variant dvp &

# Set screen layout
sh ~/.screenlayout/og-main-left-portrait.sh

# Starting autokey
#autokey &

# Starting conky
#conky &

# Setting background
xsetroot -solid "#000000" &
#xloadimage -onroot -fullscreen ~/Downloads/bg.jpg
